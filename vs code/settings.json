{
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "workbench.colorTheme": "Atom Material Theme",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": ["#373b41", "#ffffff"],
    "vim.statusBarColors.insert": ["#9955ee", "#263238"],
    "workbench.colorCustomizations": {
        "statusBar.background": "#373b41",
        "statusBar.noFolderBackground": "#373b41",
        "statusBar.debuggingBackground": "#373b41",
        "statusBar.foreground": "#ffffff"
    },
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Fira Code'",
    "editor.fontSize": 15,
    "editor.minimap.enabled": false,
    "workbench.tree.indent": 6,
    "files.autoSave": "onFocusChange",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "[plaintext]": {
        "editor.wordBasedSuggestions": false,
        "editor.quickSuggestions": false,
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 80,
        "editor.lineNumbers": "off",
        "editor.renderIndentGuides": false,
    },
    "window.zoomLevel": 0,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.errors.incompleteClasspath.severity": "ignore",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.shell.linux": "/bin/zsh",
    "go.formatTool": "goimports"
}